{
    "name": "xiaolie/water-tools",
    "description": "短视频去水印的拓展包，集成各大短视频的去水印功能,最新优化更新",
    "type": "library",
    "keywords": [
        "Xiaolie",
        "water-tools"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Xiaolie",
            "email": "76101700@qq.com"
        }
    ],
    "require": {
        "php": ">= 7.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-SimpleXML": "*",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Xiaolie\\WaterTools\\": "src"
        },
        "files": [
            "config/url-validator.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Xiaolie\\Tests\\": "tests"
        }
    }
}
